{
  "rules": {
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid",
        "flights": {
          "$year": {
            "$month": {
              "$flightId": {
                ".read": "$uid === auth.uid",
                ".write": "$uid === auth.uid",
                ".validate": "newData.hasChildren(['date', 'departure', 'arrival', 'aircraft', 'blockTime', 'status'])"
              }
            }
          }
        }
      }
    },
    ".read": false,
    ".write": false
  }
}
