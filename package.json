{
  "name": "mykneeboard",
  "private": true,
  "version": "3.0.4",
  "type": "module",
  "scripts": {
    "dev": "node generate-version.js && node generate-sw.js && vite",
    "build": "node generate-version.js && node generate-sw.js && vite build",
    "preview": "vite preview",
    "vercel-build": "node generate-version.js && node generate-sw.js && vite build",
    "build:check": "vite build && echo 'Build completed. Checking file hashes...' && ls -la dist/assets/ | grep -E '\\.(js|css)$' | head -10"
  },
  "vercel": {
    "buildCommand": "vite build",
    "outputDirectory": "dist"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-query-persist-client": "^5.90.12",
    "@vercel/node": "^5.3.24",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "firebase": "^10.13.0",
    "firebase-admin": "^13.6.1",
    "framer-motion": "^12.23.24",
    "idb-keyval": "^6.2.2",
    "leaflet": "^1.9.4",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.7.0",
    "tailwind-scrollbar-hide": "^4.0.0",
    "xlsx": "^0.18.5"
  },
