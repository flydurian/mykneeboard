    "@tailwindcss/postcss": "^4.1.14",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/recharts": "^1.8.29",
    "autoprefixer": "^10.4.21",
    "firebase-tools": "^14.18.0",
    "postcss": "^8.5.6",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.3.3",
    "vite": "^5.4.8"
  }
